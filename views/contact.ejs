<% include partials/header %>

<body id="contact">
<div id="box">
    <div class="form-box">
        <h1>Contact Me</h1>
     
         <form action="contact" method="POST">
            <label for="name">Name</label>
            <input required='required'id="name" type="text" name="Name">
          
            <label required for="email">Email</label>
            <input required='required' id="email" type="email" name="Email">
          
            <label  required='required' for="message">Message</label>
            <textarea required='required' id="message" name="Message"></textarea>
          <input required='required' class="btn btn-primary" type="submit" value="Submit" />
        </form>
      </div>
</div>
</body>



